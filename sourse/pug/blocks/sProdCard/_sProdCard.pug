mixin sProdCardMobile(secondVariant)
	// start sProdCard
	+b.sProdCard--mobile.section.d-md-none&attributes(attributes)
		
		.container
			+e.row.row
				.col-lg-7.col-xl-8
					+e.swiper-wrap
							+e.slider--mob-js.swiper
								.swiper-wrapper
									-for(let i = 0; i < 8; i++)
										+e.slide.swiper-slide
											+e.img-wrap(data-fancybox='prod-card-gallery' data-src='img/sProdCard-img-1.jpg')
												.img-wrap-center
													+img(`img/sProdCard-img-1.jpg`, 'bg')
								+e.slider-info.d-none.d-sm-flex
									+e.slider-count
										span.current-slide 01
										span.all-slides /00
									+e.pagination-wrap
										.swiper-pagination
							+e.main-slider-arrow-wrap.d-none.d-sm-block
								+arrow()
	// end sProdCard
